        // Attractive Farm Loader Class
        class AtmaFarmLoader {
            constructor() {
                this.loader = document.getElementById('farmLoader');
                this.thankYouScreen = document.getElementById('thankYouScreen');
                this.progressCircle = document.getElementById('progressCircle');
                this.progressPercentage = document.getElementById('progressPercentage');
                this.statusMessage = document.getElementById('statusMessage');
                this.loaderIcon = document.getElementById('loaderIcon');
                this.successCheck = document.getElementById('successCheck');

                this.currentProgress = 0;
                this.targetProgress = 0;
                this.isLoading = false;

                // Status messages for different stages
                this.statusMessages = [
                    "Connecting to Atma Nirbhar Farm...",
                    "Loading fresh desi cow milk inventory...",
                    "Preparing customer app link...",
                    "Gathering delivery routes...",
                    "Setting up farm dashboard...",
                    "Sending customer app link...",
                    "Finalizing your farm experience..."
                ];
            }

            updateProgress(percentage) {
                this.targetProgress = percentage;
                const circumference = 2 * Math.PI * 70; // radius = 70

                // Smooth animation to target percentage
                const animateProgress = () => {
                    if (this.currentProgress < this.targetProgress) {
                        this.currentProgress += 1;
                        const currentOffset = circumference - (this.currentProgress / 100) * circumference;
                        this.progressCircle.style.strokeDashoffset = currentOffset;
                        this.progressPercentage.textContent = this.currentProgress + '%';
                        requestAnimationFrame(animateProgress);
                    }
                };
                requestAnimationFrame(animateProgress);
            }

            updateStatus(message) {
                this.statusMessage.style.animation = 'none';
                this.statusMessage.offsetHeight; // Trigger reflow
                this.statusMessage.textContent = message;
                this.statusMessage.style.animation = 'status-slide 0.5s ease-in-out';
            }

            showLoader() {
                this.loader.classList.add('show');
                this.isLoading = true;
                this.startLoadingSequence();
            }

            hideLoader() {
                this.loader.classList.add('hide');
                this.isLoading = false;
                setTimeout(() => {
                    this.loader.style.display = 'none';
                }, 800);
            }

            startLoadingSequence() {
                const sequence = [
                    { progress: 5, message: this.statusMessages[0], delay: 500 },
                    { progress: 15, message: this.statusMessages[1], delay: 800 },
                    { progress: 25, message: this.statusMessages[2], delay: 600 },
                    { progress: 45, message: this.statusMessages[3], delay: 700 },
                    { progress: 65, message: this.statusMessages[4], delay: 600 },
                    { progress: 85, message: this.statusMessages[5], delay: 800 },
                    { progress: 100, message: this.statusMessages[6], delay: 500 }
                ];

                let currentStep = 0;

                const executeStep = () => {
                    if (currentStep < sequence.length && this.isLoading) {
                        const step = sequence[currentStep];

                        this.updateProgress(step.progress);
                        this.updateStatus(step.message);

                        // Special handling for 100%
                        if (step.progress === 100) {
                            setTimeout(() => {
                                this.showSuccess();
                            }, 1000);
                        }

                        currentStep++;
                        setTimeout(executeStep, step.delay);
                    }
                };

                executeStep();
            }

            showSuccess() {
                // Hide loader icon and show success check
                this.loaderIcon.style.display = 'none';
                this.successCheck.style.display = 'block';

                // Update status
                this.updateStatus('Customer app link sent successfully! ðŸŽ‰');

                // Show thank you screen after showing success
                setTimeout(() => {
                    this.showThankYou();
                }, 2000);
            }

            showThankYou() {
                // Hide loader
                this.hideLoader();

                // Show thank you screen
                setTimeout(() => {
                    this.thankYouScreen.classList.add('show');
                }, 500);
            }

            hideThankYou() {
                this.thankYouScreen.classList.remove('show');
            }
        }

        // Function to show cow image screen
        function showCowImageScreen() {
            const cowImageScreen = document.getElementById('cowImageScreen');
            console.log('ðŸ„ Showing cow image screen...');
            cowImageScreen.classList.add('show');

            // Automatically proceed to loader after 3 seconds
            setTimeout(() => {
                console.log('ðŸš€ Auto-proceeding to loader...');
                proceedToLoader();
            }, 3000);
        }

        // Function to proceed to loader from cow screen
        function proceedToLoader() {
            const cowImageScreen = document.getElementById('cowImageScreen');

            // Hide cow image screen
            cowImageScreen.classList.add('hide');

            // Start loader after transition
            setTimeout(() => {
                cowImageScreen.classList.remove('show');
                if (window.farmLoader) {
                    console.log('ðŸŒ¾ Starting Atma Nirbhar Farm loader after cow screen...');
                    window.farmLoader.showLoader();
                }
            }, 500);
        }

        // Function to proceed to main page
        function proceedToMainPage() {
            if (window.farmLoader) {
                window.farmLoader.hideThankYou();
            }

            // Hide all overlay screens
            setTimeout(() => {
                const overlays = document.querySelectorAll('.farm-loader-overlay, .thank-you-screen, .cow-image-screen');
                overlays.forEach(overlay => {
                    overlay.style.display = 'none';
                });
            }, 300);
        }

        // Initialize on page load
        document.addEventListener('DOMContentLoaded', function() {
            // Initialize farm loader but don't start it automatically
            window.farmLoader = new AtmaFarmLoader();
            console.log('ðŸŒ¾ Atma Nirbhar Farm loader initialized and ready for launch button click...');
        });

        // Add some automatic floating particles
        setInterval(() => {
            const particles = document.querySelectorAll('.farm-particle');
            particles.forEach((particle, index) => {
                const delay = index * 2;
                setTimeout(() => {
                    if (!particle.style.animationPlayState || particle.style.animationPlayState === 'running') {
                        particle.style.animationDelay = '0s';
                    }
                }, delay * 1000);
            });
        }, 20000);
    </script>
