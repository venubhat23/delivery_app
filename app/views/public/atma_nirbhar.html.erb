    <script>
        function handleImageUpload(event, imageNumber) {
            const file = event.target.files[0];
            if (file) {
                const reader = new FileReader();
                reader.onload = function(e) {
                    const imagePlaceholder = document.querySelector(`.image-${imageNumber}`);
                    imagePlaceholder.style.backgroundImage = `url(${e.target.result})`;
                    imagePlaceholder.classList.add('has-image');

                    // Update the text
                    const uploadText = imagePlaceholder.querySelector('.upload-text');
                    uploadText.textContent = `Change Image ${imageNumber}`;
                };
                reader.readAsDataURL(file);
            }
        }

        function createFirework(x, y) {
            const colors = ['#ffeb3b', '#f44336', '#4caf50', '#2196f3', '#ff9800', '#e91e63'];

            for (let i = 0; i < 20; i++) {
                const firework = document.createElement('div');
                firework.className = 'firework';
                firework.style.left = (x + Math.random() * 150 - 75) + 'px';
                firework.style.top = (y + Math.random() * 150 - 75) + 'px';
                firework.style.background = colors[Math.floor(Math.random() * colors.length)];
                firework.style.animationDelay = (Math.random() * 0.8) + 's';
                document.getElementById('fireworks').appendChild(firework);

                setTimeout(() => {
                    firework.remove();
                }, 2500);
            }
        }

        function createCracker(x, y) {
            const crackerColors = ['#ff5722', '#ff9800', '#ffeb3b', '#4caf50', '#2196f3'];

            for (let i = 0; i < 10; i++) {
                const cracker = document.createElement('div');
                cracker.className = 'cracker';
                cracker.style.left = (x + Math.random() * 80 - 40) + 'px';
                cracker.style.top = (y + Math.random() * 80 - 40) + 'px';
                cracker.style.background = crackerColors[Math.floor(Math.random() * crackerColors.length)];
                cracker.style.animationDelay = (Math.random() * 0.3) + 's';
                document.getElementById('fireworks').appendChild(cracker);

                setTimeout(() => {
                    cracker.remove();
                }, 1500);
            }
        }

        function createSparkles() {
            for (let i = 0; i < 30; i++) {
                const sparkle = document.createElement('div');
                sparkle.className = 'sparkle';
                sparkle.style.left = Math.random() * window.innerWidth + 'px';
                sparkle.style.top = Math.random() * window.innerHeight + 'px';
                sparkle.style.animationDelay = (Math.random() * 2) + 's';
                document.getElementById('fireworks').appendChild(sparkle);

                setTimeout(() => {
                    sparkle.remove();
                }, 1000);
            }
        }

        function startFireworks() {
            const fireworksEl = document.getElementById('fireworks');
            fireworksEl.classList.add('active');

            // Create fireworks
            const fireworkInterval = setInterval(() => {
                const x = Math.random() * window.innerWidth;
                const y = Math.random() * (window.innerHeight * 0.7);
                createFirework(x, y);
            }, 200);

            // Create crackers
            const crackerInterval = setInterval(() => {
                const x = Math.random() * window.innerWidth;
                const y = Math.random() * (window.innerHeight * 0.8);
                createCracker(x, y);
            }, 400);

            // Create sparkles periodically
            const sparkleInterval = setInterval(() => {
                createSparkles();
            }, 800);

            setTimeout(() => {
                clearInterval(fireworkInterval);
                clearInterval(crackerInterval);
                clearInterval(sparkleInterval);
            }, 10000);

            // Hide fireworks after 12 seconds
            setTimeout(() => {
                fireworksEl.classList.remove('active');
            }, 12000);
        }

        function showWhatsAppLoader() {
            const whatsappModal = document.getElementById('whatsappModal');
            const loadingStatus = document.getElementById('loadingStatus');

            whatsappModal.classList.add('active');

            const statusMessages = [
                'Connecting to customers...',
                'Sending inauguration messages...',
                'Notifying about new features...',
                'Processing customer responses...',
                'Finalizing notifications...'
            ];

            let messageIndex = 0;
            const messageInterval = setInterval(() => {
                if (messageIndex < statusMessages.length) {
                    loadingStatus.textContent = statusMessages[messageIndex];
                    messageIndex++;
                } else {
                    clearInterval(messageInterval);
                    whatsappModal.classList.remove('active');

                    // Show success modal
                    setTimeout(() => {
                        document.getElementById('successModal').classList.add('active');
                    }, 500);
                }
            }, 600);
        }

        function closeSuccessModal() {
            document.getElementById('successModal').classList.remove('active');

            // Show scrolling success message with fireworks
            setTimeout(() => {
                showScrollingSuccess();
            }, 500);
        }


        function createScrollFireworks(container) {
            const colors = ['#ffeb3b', '#f44336', '#4caf50', '#2196f3', '#ff9800', '#e91e63'];

            // Create multiple fireworks around the scrolling message
            for (let i = 0; i < 8; i++) {
                const firework = document.createElement('div');
                firework.className = 'scroll-firework';
                firework.style.position = 'absolute';
                firework.style.width = '6px';
                firework.style.height = '6px';
                firework.style.borderRadius = '50%';
                firework.style.background = colors[Math.floor(Math.random() * colors.length)];

                // Random position around the message
                const angle = (i / 8) * 2 * Math.PI;
                const radius = 80 + Math.random() * 40;
                const x = Math.cos(angle) * radius;
                const y = Math.sin(angle) * radius;

                firework.style.left = (100 + x) + 'px';
                firework.style.top = (100 + y) + 'px';
                firework.style.animation = `scroll-firework-burst 1.5s ease-out infinite`;
                firework.style.animationDelay = (Math.random() * 1) + 's';

                container.appendChild(firework);

                // Remove after animation
                setTimeout(() => {
                    if (firework.parentNode) {
                        firework.remove();
                    }
                }, 2000);
            }
        }

        function showScrollingSuccess() {
            const scrollingSuccess = document.getElementById('scrollingSuccess');
            const scrollFireworks = document.getElementById('scrollFireworks');

            scrollingSuccess.classList.add('active');

            // Create fireworks continuously while scrolling
            const fireworksInterval = setInterval(() => {
                createScrollFireworks(scrollFireworks);
            }, 800);

            // Stop after one complete cycle
            setTimeout(() => {
                clearInterval(fireworksInterval);
                scrollingSuccess.classList.remove('active');
                // Clear existing fireworks
                scrollFireworks.innerHTML = '';
            }, 8000);
        }

        function openCurtains() {
            const button = document.querySelector('.ribbon-inaugurate-button');
            const curtainContainer = document.getElementById('curtainContainer');
            const ribbon = document.getElementById('ribbon');
            const launchContainer = document.getElementById('launchContainer');

            // Animate button press
            button.style.transform = 'scale(0.9)';
            button.style.background = 'linear-gradient(45deg, #4caf50, #66bb6a)';
            button.disabled = true;

            // Show curtains
            setTimeout(() => {
                curtainContainer.classList.add('active');

                // Open curtains after a brief pause
                setTimeout(() => {
                    curtainContainer.classList.add('opening');

                    // Start fireworks during curtain opening
                    setTimeout(() => {
                        startFireworks();
                    }, 1500);

                    // Show ribbon cutting after curtains open
                    setTimeout(() => {
                        ribbon.classList.add('active');

                        setTimeout(() => {
                            ribbon.classList.add('cutting');
                        }, 500);

                        // Hide curtains and ribbon, then show WhatsApp loader
                        setTimeout(() => {
                            curtainContainer.classList.remove('active');
                            ribbon.classList.remove('active');

                            // Fade out original content
                            launchContainer.style.transition = 'opacity 1s ease-out';
                            launchContainer.style.opacity = '0';

                            setTimeout(() => {
                                // Show WhatsApp loader
                                showWhatsAppLoader();
                            }, 1000);

                        }, 2500);
                    }, 3000);
                }, 1000);
            }, 500);
        }

        function launchSoftware() {
            // Keep this function for backward compatibility
            openCurtains();
        }

        // Add some automatic floating particles
        setInterval(() => {
            const particles = document.querySelectorAll('.farm-particle');
            particles.forEach((particle, index) => {
                const delay = index * 2;
                setTimeout(() => {
                    if (!particle.style.animationPlayState || particle.style.animationPlayState === 'running') {
                        particle.style.animationDelay = '0s';
                    }
                }, delay * 1000);
            });
        }, 20000);
    </script>
